{
  "name": "nex-dox",
  "version": "0.1.0",
  "description": "A documentation site using Next.js and MDX",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dirheimerb/nex-dox.git"
  },
  "keywords": [
    "AI",
    "Documentation site",
    "Embeddings",
    "Embedding generation",
    "MDX",
    "Next.js",
    "OpenAI",
    "OpenAI Edge"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "pnpm run embeddings && next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "embeddings": "tsx src/lib/generate-embeddings.ts",
    "embeddings:refresh": "tsx src./lib/generate-embeddings.ts --refresh"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@supabase/supabase-js": "^2.31.0",
    "ai": "^2.1.26",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "common-tags": "^1.8.2",
    "github-slugger": "^2.0.0",
    "gpt3-tokenizer": "^1.1.5",
    "lucide-react": "^0.263.1",
    "mdast": "^3.0.0",
    "mdast-util-from-markdown": "^2.0.0",
    "mdast-util-mdx": "^3.0.0",
    "mdast-util-to-markdown": "^2.1.0",
    "mdast-util-to-string": "^4.0.0",
    "micromark-extension-mdxjs": "^2.0.0",
    "next": "13.4.12",
    "openai": "^3.3.0",
    "openai-edge": "^1.2.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "unist-builder": "^4.0.0",
    "unist-util-filter": "^5.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/common-tags": "^1.8.1",
    "@types/mdast": "^4.0.0",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.17",
    "@types/react-dom": "^18.2.7",
    "@types/yargs": "^17.0.24",
    "autoprefixer": "10.4.14",
    "dotenv": "^16.3.1",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.12",
    "postcss": "8.4.27",
    "prettier": "^3.0.0",
    "tailwindcss": "3.3.3",
    "ts-node": "^10.9.1",
    "tsx": "^3.12.7",
    "typescript": "5.1.6"
  },
  "prettier": {
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 80,
    "tabWidth": 2,
    "singleAttributePerLine": true
